'use strict';

/**
 * Module dependencies.
 */
var mongoose = require('mongoose'),
	Schema = mongoose.Schema;

/**
 * [model:name] Schema
 */
var [model:name]Schema = new Schema({
	[properties]
	[property]
	[property value='name']: {
		[att][att value='name'/]: [att value='type'/][cuote][/att]
		}[cuote]
	[/property]
	[/properties]
});

mongoose.model('[model:name]', [model:name]Schema);