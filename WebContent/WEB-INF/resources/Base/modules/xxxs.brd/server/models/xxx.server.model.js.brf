'use strict';

/**
 * Module dependencies.
 */
var mongoose = require('mongoose'),
	Schema = mongoose.Schema;

/**
 * [model value='name'/] Schema
 */
var [model:name]Schema = new Schema({
	[properties]
	[property]
	[property value='name'/]: {
		[att]
		[att value='name'/]: [att value='value'/][cuote]
		[/att]
		}[cuote]
	[/property]
	[/properties]
});

mongoose.model('[model value='name'/]', [model value='name'/]Schema);